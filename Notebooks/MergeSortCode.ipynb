def mergeSortedLists(a,b):
    l = []
    while a and b:
        if a[0] < b[0] :
            l.append(a.pop(0))
        else:
            l.append(b.pop(0))
    return l + a + b

result = mergeSortedLists(a,b) 
print(result) 
